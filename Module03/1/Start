public class Start {
    public static void main(String[] args) {
        Directory dir = new Directory();

        dir.files.add(new GraphicFile());
        for (int i = 1; i <= 10; i++) {
            dir.files.add(new AudioFile());
        }
        dir.files.add(new TextFile());
        dir.files.add(new TextFile());

    }
}
